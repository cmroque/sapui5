<mvc:View
    controllerName="testproject.testproject.controller.Detail"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:wt="testproject.testproject.control">
    <Page 
        title="{i18n>detailPageTitle}"
        showNavButton="true"
		navButtonPress=".onNavBack">
        <ObjectHeader 
            intro="{invoice>ShipperName}"
			title="{invoice>ProductName}"/>
        <wt:ProductRating 
			id="rating" 
			class="sapUiSmallMarginBeginEnd" 
			change=".onRatingChange"/>
    </Page>
</mvc:View>